{
  "name": "automarket-frontend",
  "version": "1.0.0",
  "description": "Frontend React App for AutoMarket Quito - Vehicle E-commerce Platform",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "vite preview --port 3001",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "cypress open",
    "cypress:ci": "cypress run",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "type-check": "tsc --noEmit",
    "analyze": "vite-bundle-visualizer",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:generate": "typedoc --out docs src",
    "clean": "rm -rf dist coverage docs .turbo"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "@reduxjs/toolkit": "^1.9.5",
    "react-redux": "^8.1.2",
    "@mui/material": "^5.14.7",
    "@mui/icons-material": "^5.14.7",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.5.0",
    "@tanstack/react-query": "^4.35.0",
    "react-hook-form": "^7.46.1",
    "yup": "^1.3.2",
    "@hookform/resolvers": "^3.3.1",
    "socket.io-client": "^4.6.2",
    "dayjs": "^1.11.9",
    "lodash": "^4.17.21",
    "react-dropzone": "^14.2.3",
    "react-image-gallery": "^1.3.0",
    "react-lazy-load-image-component": "^1.6.0",
    "recharts": "^2.8.0",
    "react-toastify": "^9.1.3",
    "react-helmet-async": "^1.3.0",
    "i18next": "^23.5.1",
    "react-i18next": "^13.2.2",
    "framer-motion": "^10.16.4",
    "@stripe/stripe-js": "^2.1.6",
    "@stripe/react-stripe-js": "^2.3.0",
    "@react-google-maps/api": "^2.19.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-select": "^5.7.4",
    "react-datepicker": "^4.18.0",
    "react-number-format": "^5.3.1",
    "react-intersection-observer": "^9.5.2",
    "react-use": "^17.4.0",
    "clsx": "^2.0.0",
    "uuid": "^9.0.0",
    "js-cookie": "^3.0.5",
    "dompurify": "^3.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/lodash": "^4.14.197",
    "@types/js-cookie": "^3.0.3",
    "@types/dompurify": "^3.0.2",
    "@types/react-lazy-load-image-component": "^1.6.0",
    "@types/uuid": "^9.0.3",
    "@vitejs/plugin-react": "^4.0.4",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.5",
    "vite-tsconfig-paths": "^4.2.1",
    "vite-bundle-visualizer": "^0.10.0",
    "typescript": "^5.2.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "vitest": "^0.34.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/user-event": "^14.4.3",
    "@vitest/coverage-v8": "^0.34.3",
    "jsdom": "^22.1.0",
    "cypress": "^13.1.0",
    "@cypress/react": "^8.0.0",
    "@cypress/webpack-dev-server": "^3.5.2",
    "@storybook/react": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "msw": "^1.3.0",
    "@faker-js/faker": "^8.0.2",
    "typedoc": "^0.25.0",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
