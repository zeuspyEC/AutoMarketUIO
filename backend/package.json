{
  "name": "automarket-backend",
  "version": "1.0.0",
  "description": "Backend API for AutoMarket Quito - Vehicle E-commerce Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:status": "knex migrate:status",
    "migrate:deploy": "knex migrate:latest --env production",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "docs:generate": "typedoc --out docs src",
    "clean": "rm -rf dist coverage docs",
    "validate": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "knex": "^2.5.1",
    "objection": "^3.1.1",
    "redis": "^4.6.7",
    "ioredis": "^5.3.2",
    "bull": "^4.11.3",
    "socket.io": "^4.6.2",
    "amqplib": "^0.10.3",
    "axios": "^1.5.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "uuid": "^9.0.0",
    "dayjs": "^1.11.9",
    "lodash": "^4.17.21",
    "joi": "^17.10.1",
    "dotenv": "^16.3.1",
    "winston": "^3.10.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "@elastic/elasticsearch": "^8.9.0",
    "nodemailer": "^6.9.4",
    "handlebars": "^4.7.8",
    "stripe": "^13.5.0",
    "twilio": "^4.16.0",
    "prom-client": "^14.2.0",
    "opentelemetry-api": "^1.4.1",
    "@opentelemetry/sdk-node": "^0.41.2",
    "@opentelemetry/auto-instrumentations-node": "^0.38.0",
    "minio": "^7.1.3",
    "slugify": "^1.6.6",
    "sanitize-html": "^2.11.0",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/morgan": "^1.9.5",
    "@types/multer": "^1.4.7",
    "@types/lodash": "^4.14.197",
    "@types/uuid": "^9.0.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/nodemailer": "^6.4.9",
    "@types/amqplib": "^0.10.1",
    "@types/speakeasy": "^2.0.7",
    "@types/qrcode": "^1.5.1",
    "@types/sanitize-html": "^2.9.0",
    "@types/jest": "^29.5.4",
    "@types/supertest": "^2.0.12",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "nodemon": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "typedoc": "^0.25.0",
    "@faker-js/faker": "^8.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "api",
    "backend",
    "express",
    "typescript",
    "postgresql",
    "redis",
    "microservices",
    "ecommerce",
    "vehicles"
  ],
  "author": "AutoMarket Quito Team",
  "license": "MIT"
}
